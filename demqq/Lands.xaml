<Window x:Class="demqq.Lands"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demqq"
        mc:Ignorable="d"
        Title="Lands" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="56*"/>
            <RowDefinition Height="296*"/>
            <RowDefinition Height="67*"/>
        </Grid.RowDefinitions>
        <Grid Background="Pink" Panel.ZIndex="-2"></Grid>
        <TextBlock Text="Lands" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25"></TextBlock>
        <Grid Grid.Row="2" Background="Aqua"></Grid>
        <DataGrid x:Name="Gland" HorizontalAlignment="Left" Height="276" Margin="10,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="772" SelectionChanged="DataGrid_SelectionChanged"/>
        <Button Content="Back" Background="Blue" HorizontalAlignment="Left" Margin="670,24,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>


    </Grid>
</Window>
