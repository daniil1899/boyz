<Window x:Class="demqq.Client"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:demqq"
        mc:Ignorable="d"
        Title="Client" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="340*"/>
            <RowDefinition Height="54*"/>
        </Grid.RowDefinitions>
        <Button Content="Back" Background="Blue" HorizontalAlignment="Left" Margin="10,15,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Grid.Row="2" Height="20"/>
        <Button Content="Rieltor" Background="Blue" HorizontalAlignment="Left" Margin="108,15,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1" Grid.Row="2" Height="20"/>
        <DataGrid x:Name="CLI" HorizontalAlignment="Left" Height="319" Margin="10,12,0,0" VerticalAlignment="Top" Width="772" SelectionChanged="CLI_SelectionChanged" Grid.Row="1"/>
        <Grid Background="Pink" Panel.ZIndex="-2" ></Grid>
        <TextBlock Text="Client" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="20"></TextBlock>
        <Grid Grid.Row="2" Background="Azure" Panel.ZIndex="-2"></Grid>

    </Grid>
</Window>
